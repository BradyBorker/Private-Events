<div class='list-events'>
        <div><%= link_to event.title, event_path(event) %></div>
        <div><%= event.date %></div>
        <div><%= event.body %></div>
        <% if user_signed_in? %>
                <%= link_to 'Attend Event',  attended_events_path(attended_event: { user_id: current_user.id, event_id: event }), data: { turbo_method: :post } %>
        <% end %>
</div>